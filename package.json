{
  "name": "ui-web-editor",
  "version": "0.0.1",
  "description": "Real-time web-document editor",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start": "npm run dev:serve",
    "dev:build": "webpack --config build/webpack.config.dev.build.js",
    "dev:serve": "webpack-dev-server --progress --config build/webpack.config.dev.serve.js",
    "prod:build": "webpack --config build/webpack.config.prod.build.js",
    "prod:serve": "webpack-dev-server --progress --config build/webpack.config.prod.serve.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "test": "jest --config test/jest.config.js"
  },
  "dependencies": {
    "firebase": "~5.8.2",
    "lodash": "~4.17.19",
    "material-design-lite": "~1.3.0",
    "moment": "~2.24.0",
    "rxjs": "~6.4.0",
    "vue": "^2.6.10",
    "vue-router": "~3.0.2"
  },
  "devDependencies": {
    "@babel/core": "~7.2.2",
    "@babel/plugin-proposal-class-properties": "~7.3.0",
    "@babel/preset-env": "~7.2.3",
    "@vue/test-utils": "~1.0.0-beta.28",
    "babel-core": "~7.0.0-bridge.0",
    "babel-eslint": "~10.0.1",
    "babel-jest": "~24.5.0",
    "babel-loader": "~7.1.5",
    "copy-webpack-plugin": "~4.6.0",
    "css-loader": "~1.0.1",
    "eslint": "~5.11.1",
    "eslint-config-standard": "~12.0.0",
    "eslint-loader": "~2.1.1",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-node": "~8.0.0",
    "eslint-plugin-promise": "~4.0.1",
    "eslint-plugin-standard": "~4.0.0",
    "eslint-plugin-vue": "~5.1.0",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "html-webpack-plugin": "~3.2.0",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~24.5.0",
    "jest-serializer-vue": "~2.0.2",
    "mini-css-extract-plugin": "~0.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "uglifyjs-webpack-plugin": "~2.1.2",
    "vue-jest": "~3.0.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2",
    "webpack-merge": "~4.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
